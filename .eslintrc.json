{
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
      "max-len": [0, 1000],
      "@typescript-eslint/no-unused-vars": 2,
      "semi": ["warn", "always"],
      "linebreak-style": ["warn", "windows"]
    }
  }
